        // Portion of expandedUrl that comes after displayUrl
        afterDisplayUrl: expandedUrl.substr(displayUrlIndex + displayUrlSansEllipses.length),
        precedingEllipsis: displayUrl.match(/^/) ? "" : "",
        followingEllipsis: displayUrl.match(/$/) ? "" : ""
