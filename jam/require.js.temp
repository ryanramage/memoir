define('js/quick', [
    'jquery',
    'underscore',
    'async',
    'handlebars',
    'couchr',
    'bird-down',
    'js/reference',
    'EpicEditor',
    'moment',
    'jquery-lifestream',
    'spin-js',
    'hbt!templates/quick',
    'hbt!templates/quick_tag',
    'hbt!templates/quick_people',
    'hbt!templates/quick_people_row',
